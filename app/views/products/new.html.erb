<h1>New Product</h1>

<div class="col-md-6">
    <%= form_with model: @product, url: {:action=> 'create'}, method: :post, local: true do |f| %>
       
        <%= render(:partial=>'form' , :locals=>{:f => f}) %>
         
        
        <div class="form-group mb-3">
            <%= submit_tag 'Create' , class:'btn btn-primary' %>
        </div>
    <% end %>
</div>
<%= link_to 'Back', products_path %>
